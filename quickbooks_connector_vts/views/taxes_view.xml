<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_taxes_filter_inherit" model="ir.ui.view">
        <field name="name">taxes.filter.inherit</field>
        <field name="model">account.tax</field>
        <field name="inherit_id" ref="account.view_account_tax_search"/>
        <field name="arch" type="xml">
            <xpath expr="//filter[1]" position="after">
                <filter string="Quickbook Taxes" name="quickbook_taxes" domain="[('qck_instance_id', '!=', False)]"/>
                <filter string="Quickbook Taxes" name="group_by_Quickbook" context="{'group_by':'qck_instance_id'}"/>
            </xpath>
        </field>
    </record>
</odoo>