<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_qbo_account_form" model="ir.ui.view">
        <field name="name">qbo.account.vts.form</field>
        <field name="model">qbo.account.vts</field>
        <field name="arch" type="xml">
            <form string="Quickbooks Accounts">
                <sheet>
                    <group>
                        <field name="quickbook_instance_id" readonly="1"/>
                        <field name="quickbook_account_id" readonly="1"/>
                        <field name="quickbook_account_name" readonly="1"/>
                        <field name="account_id"/>
                        <field name="company_id"/>
                        <field name="qbo_response" readonly="1"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="view_qbo_account_list" model="ir.ui.view">
        <field name="name">qbo.account.vts.list</field>
        <field name="model">qbo.account.vts</field>
        <field name="arch" type="xml">
            <list string="Quickbooks Account" editable="bottom">
                <field name="quickbook_instance_id" readonly="1"/>
                <field name="quickbook_account_id" readonly="1"/>
                <field name="quickbook_account_name" readonly="1"/>
                <field name="account_id"/>
                <button name="account_mapping_view" type="object" string="View" class="btn btn-link"/>
            </list>
        </field>
    </record>
</odoo>