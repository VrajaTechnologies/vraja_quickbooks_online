<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="view_qbo_taxes_form" model="ir.ui.view">
        <field name="name">qbo.taxes.vts.form</field>
        <field name="model">qbo.taxes.vts</field>
        <field name="arch" type="xml">
            <form string="Quickbooks Taxes">
                <sheet>
                    <group>
                        <field name="quickbook_tax_id" readonly="1"/>
                        <field name="quickbook_tax_name" readonly="1"/>
                        <field name="partner_id"/>
                        <field name="company_id"/>
                        <field name="qbo_response" readonly="1"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="view_qbo_tax_list" model="ir.ui.view">
        <field name="name">qbo.taxes.vts.list</field>
        <field name="model">qbo.taxes.vts</field>
        <field name="arch" type="xml">
            <list string="Quickbooks Taxes" editable="bottom">
                <field name="quickbook_tax_id" readonly="1"/>
                <field name="quickbook_tax_name" readonly="1"/>
                <field name="partner_id"/>
                <button name="taxes_mapping_view" type="object" string="View" class="btn btn-link"/>
            </list>
        </field>
    </record>
</odoo>