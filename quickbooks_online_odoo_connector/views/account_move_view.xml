<odoo>
    <record id="view_account_move_export_error_filter" model="ir.ui.view">
        <field name="name">account.move.filter.inherit</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_account_invoice_filter"/>
        <field name="arch" type="xml">
            <xpath expr="//filter[@name='late']" position="after">
                <separator/>
                <filter string="Quickbooks Export Issue"
                        name="quickbook_invoices_and_vendor_bill"
                        domain="[('error_in_export', '=', True)]"/>
            </xpath>
            <xpath expr="//filter[@name='group_by_company']" position="after">
                <separator/>
                <filter string="Quickbooks Export Issue"
                        name="group_by_Quickbook"
                        context="{'group_by':'error_in_export'}"/>
                <separator/>
            </xpath>
        </field>
    </record>
</odoo>