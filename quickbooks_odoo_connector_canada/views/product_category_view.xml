<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
<record id="quickbook_product_category_filter" model="ir.ui.view">
        <field name="name">vendor.filter.inherit</field>
        <field name="model">product.category</field>
        <field name="inherit_id" ref="product.product_category_search_view"/>
        <field name="arch" type="xml">
            <xpath expr="//search" position="inside">
                <separator/>
                <filter string="Quickbook Product Category"
                        name="quickbook_product_category"
                        domain="[('qck_instance_id', '!=', False)]"/>
            </xpath>
            <xpath expr="//search" position="inside">
                <separator/>
                <filter string="Quickbooks Product Category" name="group_by_quickbook_product_category"
                        context="{'group_by':'qck_instance_id'}"/>
            </xpath>
        </field>
    </record>
</odoo>